<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.henghao.entity.wzp.TorHorizonUserDeptEntity" table="tor_horizon_user_dept" schema="hz">
        <id name="id" column="ID"/>
        <property name="userId" column="USER_ID"/>
        <property name="deptId" column="DEPT_ID"/>
        <property name="joinTime" column="JOIN_TIME"/>
        <property name="quitTime" column="QUIT_TIME"/>
        <property name="active" column="ACTIVE"/>
        <property name="orderNo" column="ORDER_NO"/>
        <property name="isdefault" column="ISDEFAULT"/>
    </class>
</hibernate-mapping>